{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "logs": "firebase functions:log",
    "validate": "tsc && npm run lint",
    "lint": "tslint --project tsconfig.json",
    "server": "npm run build && firebase emulators:start --only functions",
    "deploy:prod": "firebase use trulink-9c2e2 && firebase deploy --only functions",
    "deploy:dev": "firebase use trulink-dev-c8384 && firebase deploy --only functions",
    "deploy-api:prod": "firebase use trulink-9c2e2 && firebase deploy --only functions:api",
    "deploy-api:dev": "firebase use trulink-dev-c8384 && firebase deploy --only functions:api",
    "serve": "nodemon index.js",
    "start": "node index.js"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
